Every 0$1 trigger game do
SpawnCrates([crates_west, crates_east, crates_north_west, crates_east], 0$50);

Export Function SpawnCrates(areas, time);
var i, p;
begin
if not areas then
   exit;

repeat
 wait(time);

 p := rand(1, 90);

 for i in areas do
     begin

     if Prob(p) then
        begin
        CreateCratesArea(rand(1,5), i, true);
        wait(rand(0$13, 0$17));
        end;

     end;

 time := time + 0$3;
until not game;
End;