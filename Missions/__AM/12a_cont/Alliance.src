Export JMM, Roth;

Export Function PrepareAlliance;
begin
uc_side := 7;
uc_nation := 1;

JMM := PrepareUnit('JMM', (not debug), '11_');
Roth := NewCharacter('Roth');

SetDir(JMM, 1);
PlaceUnitXY(JMM, 105, 10, false);

SetDir(Roth, 4);
PlaceUnitXY(Roth, 109, 10, false);

ComTurnUnit(JMM, Roth);
ComTurnUnit(Roth, JMM);
End;