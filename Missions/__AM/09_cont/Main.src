// Siberit!
//
// 9 misia by Serpent
//
Starting
begin
ResetFog;

Init;
DebugMode;
PrepareNature;
PrepareAmerican;
PrepareOvsyenko;
Action;
End;

Export mission_prefix, debug, terminal;
Export jmm_in_ovsyenko, powell_want_sib, powell_want_sib_counter;
Function Init;
begin
debug := true;
jmm_in_ovsyenko := false;
mission_prefix := '09_';
powell_want_sib := false;
powell_want_sib_counter := 15$00;
End;

Function DebugMode;
begin
if not debug then
   exit;

FogOff(1);
End;